(require 'elk-test)

(deftest "Sorted keywords"
  (require 'company-keywords)
  (dolist (pair company-keywords-alist)
    (when (consp (cdr pair))
      (let ((prev (cadr pair)))
        (dolist (next (cddr pair))
          (assert-not-equal prev next)
          (assert-string< prev next)
          (setq prev next))))))
